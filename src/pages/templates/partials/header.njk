{% set links = datasets.get('site-map') %}

<header class="container">
  <div class="row">
    <div class="col-12">
      
      <ul class="header">
        {% for link in links %}
        <li class="header__item">
          <a class="header__link" href="{{ link.ref }}">{{ link.name }}</a>
        </li>
        {% endfor %}
      </ul>

    </div>
  </div>
</header>
